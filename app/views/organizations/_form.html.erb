<%= form_with model: @organization do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <br>

    <%= f.label :website %>
    <%= f.text_field :website %><br>
    <br>

    <%= f.label :phone %>
    <%= f.text_field :phone %><br>
    <br>

    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <br>

    <%= f.submit 'Submit' %>
<% end %> 

